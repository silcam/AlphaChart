{
  "name": "alphachart-server",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/express": "^4.17.0",
    "babel-core": "6.14.0",
    "express": "4.13.3",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "start": "concurrently -p \"[{name}]\" -n \"TS,Server,Client\"  -c yellow,green,magenta \"tsc -w\" \"yarn server\" \"yarn client\"",
    "server": "nodemon server/dist/server.js --watch server/dist",
    "client": "node start-client.js"
  },
  "devDependencies": {
    "concurrently": "3.1.0",
    "nodemon": "^1.19.1"
  }
}
