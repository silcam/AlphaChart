{
  "name": "alphachart-server",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/express": "^4.17.0",
    "@types/mongodb": "^3.1.31",
    "babel-core": "6.14.0",
    "body-parser": "^1.19.0",
    "express": "4.13.3",
    "immutability-helper": "^3.0.1",
    "mongodb": "^3.3.0-beta2",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "start": "concurrently -p \"[{name}]\" -n \"TS,Server,Client\"  -c yellow,green,magenta \"tsc -w\" \"yarn start-server\" \"yarn start-client\"",
    "start-server": "nodemon server/dist/server/src/server.js --watch server/dist/",
    "start-client": "node start-client.js"
  },
  "devDependencies": {
    "concurrently": "3.1.0",
    "nodemon": "^1.19.1"
  }
}
