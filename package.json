{
  "name": "alphachart-server",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@types/cookie-session": "^2.0.37",
    "@types/express": "^4.17.0",
    "@types/express-fileupload": "^1.1.0",
    "@types/jest": "^24.0.18",
    "@types/mongodb": "^3.1.31",
    "@types/node-cleanup": "^2.1.1",
    "@types/supertest": "^2.0.8",
    "babel-core": "6.14.0",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.3.3",
    "express": "4.13.3",
    "express-fileupload": "^1.1.5",
    "immutability-helper": "^3.0.1",
    "memfs": "^2.15.5",
    "migrate": "^1.6.2",
    "mongodb": "^3.3.0-beta2",
    "node-cleanup": "^2.1.2",
    "supertest": "^4.0.2",
    "typescript": "^3.5.3"
  },
  "scripts": {
    "start": "concurrently -p \"[{name}]\" -n \"TS,Server,Client\"  -c yellow,green,magenta \"tsc -w\" \"yarn start-server\" \"yarn start-client\"",
    "start-server": "nodemon server/dist/server/src/server.js --watch server/dist/",
    "start-client": "node start-client.js",
    "test": "NODE_ENV=test jest --watchAll",
    "start-test": "yarn reset-cypress-images && NODE_ENV=test-cypress yarn start",
    "deploy": "cd deploy && ./deploy.sh",
    "reset-cypress-images": "node reset-cypress-images.js"
  },
  "devDependencies": {
    "@cypress/snapshot": "^2.1.3",
    "concurrently": "3.1.0",
    "cypress": "^3.5.0",
    "cypress-file-upload": "^3.4.0",
    "cypress-image-snapshot": "^3.1.1",
    "jest": "24.7.1",
    "nodemon": "^1.19.1",
    "ts-jest": "^24.1.0"
  }
}
